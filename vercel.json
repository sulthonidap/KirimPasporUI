{
    "headers": [
      {
        "source": "/:path*.png",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      },
      {
        "source": "/:path*.jpeg",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  